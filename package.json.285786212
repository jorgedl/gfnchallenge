{
    "name": "geofusion-ui",
    "version": "1.0.0",
    "description": "Desafio t√©cnico da Geofusion",
    "main": "index.js",
    "scripts": {
        "test": "./node_modules/.bin/jest",
        "build": "NODE_ENV=production webpack",
        "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --content-base src --inline --hot",
        "start-mock-server": "./node_modules/.bin/nodemon ./mock_server/mockServer.js"
    },
    "keywords": [
        "geofusion"
    ],
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/setupTests.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "collectCoverageFrom": [
            "./src/**/*.js"
        ],
        "testURL": "http://localhost/"
    },
    "author": "Jorge De Lucca",
    "license": "ISC",
    "devDependencies": {
